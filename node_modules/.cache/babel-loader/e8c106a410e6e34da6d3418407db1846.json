AAA,KAAA,EAAA;AACA,yBAAA;AACA,MAAA,OAAA,EAAA,iBAAA,MAAA,EAAA;AACA,aAAA,WAAA,CAAA,MAAA;AACA,OAHA;AAIA,MAAA,IAAA,EAAA,IAJA;AAKA,MAAA,SAAA,EAAA;AALA;AADA,GAhBA;AAyBA,EAAA,OAAA,EAAA;AACA,IAAA,WAAA,EAAA,qBAAA,KAAA,EAAA;AACA,UAAA,KAAA,EAAA;AACA,YAAA,SAAA,GAAA,EAAA;AACA,QAAA,SAAA,GAAA,KAAA,IAAA,CAAA,MAAA,CAAA,UAAA,IAAA,EAAA;AACA,iBAAA,IAAA,CAAA,KAAA,CAAA,WAAA,GAAA,QAAA,CAAA,KAAA,CAAA,WAAA,EAAA,CAAA;AACA,SAFA,CAAA;AAGA,aAAA,IAAA,GAAA,SAAA;AACA,OANA,MAMA;AACA,aAAA,OAAA;AACA;AACA,KAXA;AAYA,IAAA,OAZA,qBAYA;AAAA;;AAAA;AAAA;;AAAA;AAAA;AAAA;AAAA;AAEA,gBAAA,KAAA,CAAA,IAAA,GAAA,EAAA;;AACA,oBAAA,CAAA,KAAA,CAAA,MAAA,EAAA;AACA,kBAAA,KAAA,CAAA,OAAA,GAAA,IAAA;AACA;;AALA;AAAA,uBAOA,WAAA,CACA,MADA,GAEA,KAFA,CAEA,UAAA,GAAA,EAAA;AACA,kBAAA,OAAA,CAAA,GAAA,CAAA,GAAA;AACA,kBAAA,KAAA,CAAA,OAAA,GAAA,IAAA;AACA,iBALA,EAMA,OANA,CAMA,YAAA;AACA,kBAAA,KAAA,CAAA,OAAA,GAAA,KAAA;AACA,iBARA,CAPA;;AAAA;AAOA,gBAAA,IAPA;;AAAA,sBAiBA,OAAA,IAAA,KAAA,WAjBA;AAAA;AAAA;AAAA;;AAAA;;AAAA;AAmBA,oBAAA,IAAA,CAAA,IAAA,CAAA,MAAA,EAAA;AACA,kBAAA,KAAA,CAAA,IAAA,IAAA,CAAA;;AACA,gCAAA,KAAA,CAAA,IAAA,EAAA,IAAA,sCAAA,IAAA,CAAA,IAAA;;AACA,kBAAA,KAAA,CAAA,MAAA,GAAA,IAAA;AACA;;AAvBA;AAAA;AAAA;AAAA;AAAA;AAAA;AAAA;AAwBA;AApCA;AAzBA,CAAA","sourcesContent":["<template>\n  <div class=\"pt-7 px-sm-10\">\n    <v-container fluid style=\"cursor: pointer\">\n      <v-row>\n        <v-col\n          cols=\"8\"\n          sm=\"7\"\n          md=\"10\"\n          lg=\"10\"\n          v-for=\"(item, i) in loading ? 12 : news\"\n          :key=\"i\"\n          class=\"mx-lg-0 mx-md-0 mx-sm-auto mx-auto mb-4 shadow\"\n          style=\"box-shadow: 0 0 10px rgba(0,0,0,.2);\"\n        >\n          <v-skeleton-loader\n            class=\"mx-auto\"\n            type=\"list-item-avatar-three-line\"\n            :loading=\"loading\"\n            tile\n            large\n          >\n            <v-card class=\"card\" tile flat>\n              <v-row no-gutters>\n                <v-col class=\"mx-auto\" cols=\"12\" sm=\"8\" md=\"5\" lg=\"4\">\n                  <!-- <v-responsive max-height=\"100%\"> -->\n                  <v-img\n                    max-height=\"200\"\n                    class=\"align-center\"\n                    :src=\"`${item.image}`\"\n                  >\n                  </v-img>\n                  <!-- </v-responsive> -->\n                </v-col>\n                <v-col class=\"hidden-sm-and-down\">\n                  <div class=\"ml-4 \">\n                    <v-card-title class=\"pl-2 pt-0 subtitle-1 font-weight-bold\">\n                      {{ item.title }}\n                    </v-card-title>\n                    <v-card-subtitle class=\"pl-2 pb-0\">\n                      {{ item.author }}\n                      <v-icon>mdi-circle-small</v-icon\n                      >{{ item.views }}\n                      {{ item.date }}\n                    </v-card-subtitle>\n                    <v-card-subtitle class=\"pl-2 pt-0\">\n                      {{ item.body }}\n                    </v-card-subtitle>\n                    <v-card-subtitle class=\"pl-2 pt-0\">\n                      {{ item.url }}\n                    </v-card-subtitle>\n                  </div>\n                </v-col>\n              </v-row>\n            </v-card>\n          </v-skeleton-loader>\n        </v-col>\n        <v-col class=\"text-center\" v-if=\"news.length === 0 && !loading\">\n          <p>No news yet</p>\n        </v-col>\n        <v-col cols=\"12\" sm=\"12\" md=\"12\" lg=\"12\">\n          <infinite-loading @infinite=\"getNews\">\n            <div slot=\"spinner\">\n              <v-progress-circular\n                indeterminate\n                color=\"red\"\n              ></v-progress-circular>\n            </div>\n            <div slot=\"no-results\"></div>\n            <span slot=\"no-more\"></span>\n            <div slot=\"error\" slot-scope=\"{ trigger }\">\n              <v-alert prominent type=\"error\">\n                <v-row align=\"center\">\n                  <v-col class=\"grow\">\n                    <div class=\"title\">Error!</div>\n                    <div>\n                      Something went wrong, but don’t fret — let’s give it\n                      another shot.\n                    </div>\n                  </v-col>\n                  <v-col class=\"shrink\">\n                    <v-btn @click=\"trigger\">Take action</v-btn>\n                  </v-col>\n                </v-row>\n              </v-alert>\n            </div>\n          </infinite-loading>\n        </v-col>\n      </v-row>\n    </v-container>\n  </div>\n</template>\n\n<script>\nimport InfiniteLoading from 'vue-infinite-loading'\n\nimport { mapGetters } from 'vuex'\nimport NewsService from '@/services/NewsService'\n\nexport default {\n  name: 'News',\n  components: {\n    InfiniteLoading\n  },\n  data: () => ({\n    loading: false,\n    loaded: false,\n    errored: false,\n    news: [],\n    page: 1,\n    searchName: \"\"\n  }),\n  computed: {\n    ...mapGetters(['currentUser', 'getUrl', 'isAuthenticated'])\n  },\n  watch: { \n  '$route.query.name': {\n      handler: function(search) {\n        this.getDataById(search)\n      },\n      deep: true,\n      immediate: true\n    }\n  },\n  methods: {\n    getDataById: function(value) {\n      if (value) {\n        let newsArray = [];\n        newsArray = this.news.filter(item => {\n          return item.title.toLowerCase().includes(value.toLowerCase())\n        });\n        this.news = newsArray;\n      } else {\n        this.getNews();\n      }\n    },\n    async getNews() {\n\n      this.news = [];\n      if (!this.loaded) {\n        this.loading = true\n      }\n\n      const news = await NewsService\n        .getAll()\n        .catch((err) => {\n          console.log(err)\n          this.errored = true\n        })\n        .finally(() => {\n          this.loading = false\n        })\n\n      if (typeof news === 'undefined') return\n\n      if (news.data.length) {\n        this.page += 1\n        this.news.push(...news.data)\n        this.loaded = true\n      }\n    }\n  }\n}\n</script>\n\n<style lang=\"scss\">\n.shadow {\n   background-color: #f5f5f5;\n  }\n\n  .shadow:hover {\n   background-color: #d2eef3;\n  }\n.v-skeleton-loader__list-item-avatar-three-line {\n  height: 250px;\n  .v-skeleton-loader__paragraph.v-skeleton-loader__bone {\n    margin-top: -125px;\n    @media (max-width: 700px) {\n      display: none;\n      margin-top: inherit;\n    }\n  }\n  .v-skeleton-loader__avatar {\n    height: 200px !important;\n    width: 300px !important;\n  }\n  .v-skeleton-loader__text {\n    height: 20px;\n  }\n}\n</style>"],"sourceRoot":"src/views"}]}